generator client {
  provider = "prisma-client-js"
  output   = "../src/infra/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id           String   @id @default(uuid()) @db.Uuid
  name         String
  price        Float
  description  String
  quantity     Int
  date_created DateTime @default(now())

  @@map("products")
}
